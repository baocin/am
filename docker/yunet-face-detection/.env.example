# YuNet Face Detection Service Configuration

# Service Configuration
PORT=8000
LOG_LEVEL=INFO
DEBUG=false

# Performance
MAX_WORKERS=4
TIMEOUT=300
REQUEST_LIMIT=100

# Model Configuration
MODEL_DIR=/app/models
MODEL_NAME=face_detection_yunet_2023mar_int8.onnx
AUTO_DOWNLOAD_MODELS=true

# Detection Parameters
SCORE_THRESHOLD=0.7
NMS_THRESHOLD=0.3
TOP_K=5000

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090